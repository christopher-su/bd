<!DOCTYPE html>
<html>
<head>
  <%= stylesheet_link_tag 'gl_page', :media => 'screen' %>
</head>
<body>

    <div>
      <%= link_to "返回",pages_admin_path %> | <%= link_to "新增公司成员",new_team_member_path %>
    </div>
    <hr/>
    <div>
      <table>
        <tr>
          <td>用户名</td>
          <td>创建时间</td>
          <td></td>
        </tr>
        <% @teamMembers.each do |t| %>
            <tr>
              <td><%= t.name %></td>
              <td><%= t.created_at %></td>
              <td><%= link_to "删除",t,:method => "DELETE",:confirm => "你确定要删除该公司成员吗？" %></td>
            </tr>
        <% end %>
      </table>
    </div>

</body>
</html>


